{
    "name": "@softvisio/core",
    "version": "3.11.0",
    "description": "Softisio Core",
    "keywords": [
        "softvisio",
        "core"
    ],
    "homepage": "https://softvisio.github.io/core/",
    "bugs": {
        "url": "https://github.com/softvisio/core/issues",
        "email": "zdm@softvisio.net"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/softvisio/core.git"
    },
    "license": "ISC",
    "author": "zdm <zdm@softvisio.net>",
    "type": "module",
    "imports": {
        "#index": "@softvisio/core",
        "#browser/*": "./lib/_browser/*.js",
        "#lib/*": "@softvisio/core/*",
        "#resources/*": "./resources/*"
    },
    "exports": {
        ".": {
            "browser": "./lib/_browser/index.js",
            "node": "./lib/index.js"
        },
        "./*": "./lib/*.js",
        "./resources/*": "./resources/*",
        "./api": {
            "browser": "./lib/_browser/api.js",
            "node": "./lib/api.js"
        },
        "./blob": {
            "browser": "./lib/_browser/blob.js",
            "node": "./lib/blob.js"
        },
        "./const": {
            "browser": "./lib/_browser/const.js",
            "node": "./lib/const.js"
        },
        "./env": {
            "browser": "./lib/_browser/env.js",
            "node": "./lib/env.js"
        },
        "./events": {
            "browser": "./lib/_browser/events.js",
            "node": "./lib/events.js"
        },
        "./fetch": {
            "browser": "./lib/_browser/fetch.js",
            "node": "./lib/fetch.js"
        },
        "./file": {
            "browser": "./lib/_browser/file.js",
            "node": "./lib/file.js"
        },
        "./form-data": {
            "browser": "./lib/_browser/form-data.js",
            "node": "./lib/form-data.js"
        },
        "./utils": {
            "browser": "./lib/_browser/utils.js",
            "node": "./lib/utils.js"
        },
        "./websocket": {
            "browser": "./lib/_browser/websocket.js",
            "node": "./lib/websocket.js"
        }
    },
    "dependencies": {
        "@softvisio/argon2": "^1.0.0",
        "@softvisio/globals": "^3.0.0",
        "@softvisio/msgpack": "^3.0.0",
        "@softvisio/result": "^2.0.0",
        "@softvisio/uws": "^1.0.0",
        "ajv": "^8.0.1",
        "ajv-errors": "^3.0.0",
        "ajv-formats": "^2.0.0",
        "ajv-formats-draft2019": "^1.4.3",
        "ajv-keywords": "^5.0.0",
        "bufferutil": "^4.0.3",
        "busboy": "^0.3.1",
        "ejs": "^3.1.5",
        "eventemitter3": "^4.0.7",
        "fast-xml-parser": "^3.17.6",
        "form-data": "^4.0.0",
        "glob": "^7.1.6",
        "ip-num": "^1.3.0",
        "js-yaml": "^4.0.0",
        "json5": "^2.2.0",
        "lru-cache": "^6.0.0",
        "mime-db": "^1.45.0",
        "node-fetch": "^2.6.1",
        "redis": "^3.0.2",
        "utf-8-validate": "^5.0.4",
        "uuid": "^8.3.2",
        "wrap-ansi": "^8.0.0",
        "ws": "^7.4.2"
    },
    "engines": {
        "node": ">=16.0.0",
        "npm": ">=7.10.0"
    }
}
