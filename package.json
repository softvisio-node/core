{
    "name": "@softvisio/core",
    "version": "7.77.0",
    "description": "Softisio core",
    "keywords": [
        "softvisio",
        "core"
    ],
    "homepage": "https://softvisio-node.github.io/core/",
    "bugs": {
        "url": "https://github.com/softvisio-node/core/issues",
        "email": "zdm <zdm@softvisio.net>"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/softvisio-node/core.git"
    },
    "license": "ISC",
    "author": "zdm <zdm@softvisio.net>",
    "type": "module",
    "imports": {
        "#browser/*": "./lib/_browser/*.js",
        "#lib/*": "@softvisio/core/*",
        "#resources/*": "./resources/*"
    },
    "exports": {
        "./*": "./lib/*.js",
        "./app.yaml": "./lib/app/app.yaml",
        "./resources/*": "./resources/*",
        "./api": {
            "browser": "./lib/_browser/api.js",
            "node": "./lib/api.js"
        },
        "./app/constants": {
            "browser": "./lib/_browser/app/constants.js",
            "node": "./lib/app/constants.js"
        },
        "./app/user/permissions": {
            "browser": "./lib/_browser/app/user/permissions.js",
            "node": "./lib/app/user/permissions.js"
        },
        "./blob": {
            "browser": "./lib/_browser/blob.js",
            "node": "./lib/blob.js"
        },
        "./env": {
            "browser": "./lib/_browser/env.js",
            "node": "./lib/env.js"
        },
        "./fetch": {
            "browser": "./lib/_browser/fetch.js",
            "node": "./lib/fetch.js"
        },
        "./file": {
            "browser": "./lib/_browser/file.js",
            "node": "./lib/file.js"
        },
        "./form-data": {
            "browser": "./lib/_browser/form-data.js",
            "node": "./lib/form-data.js"
        },
        "./utils": {
            "browser": "./lib/_browser/utils.js",
            "node": "./lib/utils.js"
        },
        "./uuid": {
            "browser": "./lib/_browser/uuid.js",
            "node": "./lib/uuid.js"
        },
        "./websocket": {
            "browser": "./lib/_browser/websocket.js",
            "node": "./lib/websocket.js"
        }
    },
    "bin": {
        "nenv": "bin/nenv.js"
    },
    "files": [
        "/bin",
        "/lib",
        "/resources",
        "/src"
    ],
    "scripts": {
        "build": "node bin/build.js",
        "install": "node bin/install.js"
    },
    "dependencies": {
        "@mtproto/core": "^6.3.0",
        "@softvisio/argon2": "^1.2.5",
        "@softvisio/msgpack": "^3.0.0",
        "@softvisio/sqlite": "^1.15.39",
        "@softvisio/utils": "^1.1.0",
        "@softvisio/uws": "^2.0.0",
        "ajv": "^8.0.1",
        "ajv-errors": "^3.0.0",
        "ajv-formats": "^3.0.0",
        "ajv-formats-draft2019": "^1.4.3",
        "ajv-keywords": "^5.0.0",
        "bufferutil": "^4.0.3",
        "decimal.js": "^10.4.3",
        "ejs": "^3.1.5",
        "fast-xml-parser": "^4.2.2",
        "js-yaml": "^4.1.0",
        "json5": "^2.2.0",
        "luxon": "^3.0.0",
        "mime-db": "^1.45.0",
        "mmdb-lib": "^2.0.2",
        "node-forge": "^1.3.1",
        "semver": "^7.3.7",
        "ua-parser-js": "^1.0.2",
        "utf-8-validate": "^6.0.0",
        "ws": "^8.0.0"
    },
    "engines": {
        "node": ">=20.9.0",
        "npm": ">=10.1.0"
    }
}
