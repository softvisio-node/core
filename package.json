{
    "name": "@softvisio/core",
    "version": "3.4.0",
    "description": "Softisio Core",
    "keywords": [
        "softvisio",
        "core"
    ],
    "homepage": "https://bitbucket.org/softvisio/softvisio-core",
    "bugs": {
        "url": "https://bitbucket.org/softvisio/softvisio-core/issues",
        "email": "zdm@softvisio.net"
    },
    "repository": {
        "type": "git",
        "url": "https://bitbucket.org/softvisio/softvisio-core.git"
    },
    "license": "ISC",
    "author": "zdm <zdm@softvisio.net>",
    "type": "module",
    "imports": {
        "#index": "@softvisio/core",
        "#internals/*": "./lib/_internals/*.js",
        "#lib/*": "@softvisio/core/*",
        "#resources/*": "./resources/*"
    },
    "exports": {
        ".": {
            "browser": "./lib/index/browser.js",
            "node": "./lib/index.js"
        },
        "./*": "./lib/*.js",
        "./resources/*": "./resources/*",
        "./api": {
            "browser": "./lib/_intenals/api/browser.js",
            "node": "./lib/api.js"
        },
        "./const": {
            "browser": "./lib/const/browser.js",
            "node": "./lib/const.js"
        },
        "./env": {
            "browser": "./lib/env/browser.js",
            "node": "./lib/env.js"
        },
        "./events": {
            "browser": "./lib/events/browser.js",
            "node": "./lib/events.js"
        },
        "./fetch": {
            "browser": "./lib/_internals/fetch.js",
            "node": "./lib/fetch.js"
        },
        "./utils": {
            "browser": "./lib/utils/browser.js",
            "node": "./lib/utils.js"
        },
        "./websocket": {
            "browser": "./lib/_internals/websocket.js",
            "node": "./lib/websocket.js"
        }
    },
    "bin": {
        "update-databases": "bin/update-databases.js"
    },
    "scripts": {
        "lint": "softvisio-cli lint"
    },
    "dependencies": {
        "@softvisio/argon2": "^1.0.0",
        "@softvisio/globals": "^3.0.0",
        "@softvisio/msgpack": "^3.0.0",
        "@softvisio/result": "^2.0.0",
        "@softvisio/uws": "^1.0.0",
        "ajv": "^8.0.1",
        "ajv-errors": "^3.0.0",
        "ajv-formats": "^2.0.0",
        "ajv-formats-draft2019": "^1.4.3",
        "ajv-keywords": "^5.0.0",
        "ansi-colors": "^4.1.1",
        "big-integer": "^1.6.48",
        "bufferutil": "^4.0.3",
        "busboy": "^0.3.1",
        "ejs": "^3.1.5",
        "eventemitter3": "^4.0.7",
        "fast-xml-parser": "^3.17.6",
        "form-data": "^4.0.0",
        "glob": "^7.1.6",
        "ip-num": "^1.3.0",
        "js-yaml": "^4.0.0",
        "json5": "^2.2.0",
        "lru-cache": "^6.0.0",
        "luxon": "^1.25.0",
        "mime-db": "^1.45.0",
        "mmdb-lib": "^1.2.0",
        "node-fetch": "^2.6.1",
        "redis": "^3.0.2",
        "streamsearch": "^0.1.2",
        "tar": "^6.1.0",
        "utf-8-validate": "^5.0.4",
        "uuid": "^8.3.2",
        "which-country": "^1.0.0",
        "wrap-ansi": "^8.0.0",
        "ws": "^7.4.2"
    },
    "engines": {
        "node": ">=16.0.0",
        "npm": ">=7.10.0"
    },
    "lint": [
        "*.*",
        "lib/**",
        "resources/**",
        "test/**"
    ]
}
