{
    "name": "@softvisio/core",
    "version": "6.48.6",
    "description": "Softisio core",
    "keywords": [
        "softvisio",
        "core"
    ],
    "homepage": "https://softvisio.github.io/core/",
    "bugs": {
        "url": "https://github.com/softvisio/core/issues",
        "email": "zdm@softvisio.net"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/softvisio/core.git"
    },
    "license": "ISC",
    "author": "zdm <zdm@softvisio.net>",
    "type": "module",
    "imports": {
        "#browser/*": "./lib/_browser/*.js",
        "#lib/*": "@softvisio/core/*",
        "#resources/*": "./resources/*"
    },
    "exports": {
        "./*": "./lib/*.js",
        "./resources/*": "./resources/*",
        "./api": {
            "browser": "./lib/_browser/api.js",
            "node": "./lib/api.js"
        },
        "./blob": {
            "browser": "./lib/_browser/blob.js",
            "node": "./lib/blob.js"
        },
        "./env": {
            "browser": "./lib/_browser/env.js",
            "node": "./lib/env.js"
        },
        "./fetch": {
            "browser": "./lib/_browser/fetch.js",
            "node": "./lib/fetch.js"
        },
        "./file": {
            "browser": "./lib/_browser/file.js",
            "node": "./lib/file.js"
        },
        "./form-data": {
            "browser": "./lib/_browser/form-data.js",
            "node": "./lib/form-data.js"
        },
        "./utils": {
            "browser": "./lib/_browser/utils.js",
            "node": "./lib/utils.js"
        },
        "./uuid": {
            "browser": "./lib/_browser/uuid.js",
            "node": "./lib/uuid.js"
        },
        "./websocket": {
            "browser": "./lib/_browser/websocket.js",
            "node": "./lib/websocket.js"
        }
    },
    "bin": {
        "nenv": "bin/nenv.js",
        "update-core": "bin/update.js"
    },
    "scripts": {
        "build": "node bin/update.js --build",
        "install": "node bin/update.js",
        "update": "node bin/update.js"
    },
    "dependencies": {
        "@softvisio/argon2": "^1.2.5",
        "@softvisio/msgpack": "^3.0.0",
        "@softvisio/result": "^5.0.0",
        "@softvisio/uws": "^2.0.0",
        "ajv": "^8.0.1",
        "ajv-errors": "^3.0.0",
        "ajv-formats": "^2.0.0",
        "ajv-formats-draft2019": "^1.4.3",
        "ajv-keywords": "^5.0.0",
        "bufferutil": "^4.0.3",
        "ejs": "^3.1.5",
        "glob": "^7.1.6",
        "js-yaml": "^4.1.0",
        "json5": "^2.2.0",
        "luxon": "^2.1.1",
        "mime-db": "^1.45.0",
        "tar": "^6.1.11",
        "utf-8-validate": "^5.0.4",
        "ws": "^8.0.0"
    },
    "engines": {
        "node": ">=16.11.0",
        "npm": ">=8.0.0"
    }
}
